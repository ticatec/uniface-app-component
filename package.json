{
  "name": "@ticatec/uniface-app-component",
  "version": "0.1.0",
  "description": "uniface web app components base on the uniface web elements",
  "scripts": {
    "dev": "vite dev",
    "build:css": "sass ./src/lib/uniface-app-component.scss:./src/lib/uniface-app-component.css --no-source-map",
    "build": "npm run build:css && svelte-kit sync && svelte-package",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "publish:public": "npm run build && npm publish --access public"
  },
  "files": [
    "dist",
    "!dist/**/*.scss",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./uniface-app-component.css": {
      "default": "./dist/uniface-app-component.css"
    },
    "./AppModule": {
      "types": "./dist/module/AppModule.d.ts",
      "import": "./dist/module/AppModule.js"
    },
    "./uniAppCtx": {
      "types": "./dist/common/uniAppCtx.d.ts",
      "import": "./dist/common/uniAppCtx.js"
    },
    "./i18n_resources": {
      "types": "./dist/i18n_resources/index.d.ts",
      "import": "./dist/i18n_resources/index.js"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "import": "./dist/common/index.js"
    },
    "./ListDataManager": {
      "types": "./dist/common/ListDataManager.d.ts",
      "import": "./dist/common/ListDataManager.js"
    },
    "./HomePage": {
      "types": "./dist/HomePage.svelte.d.ts",
      "import": "./dist/HomePage.svelte"
    },
    "./card": {
      "types": "./dist/cards/index.d.ts",
      "import": "./dist/cards/index.js"
    },
    "./card/CardListBoard": {
      "types": "./dist/cards/index.d.ts",
      "import": "./dist/cards/index.js"
    },
    "./card/ListPage": {
      "types": "./dist/cards/ListPage.svelte.d.ts",
      "import": "./dist/cards/ListPage.svelte"
    },
    "./card/PagingListPage": {
      "types": "./dist/cards/PagingListPage.svelte.d.ts",
      "import": "./dist/cards/PagingListPage.svelte"
    },
    "./card/managed/ListPage": {
      "types": "./dist/cards/managed/ListPage.svelte.d.ts",
      "import": "./dist/cards/managed/ListPage.svelte"
    },
    "./card/managed/PagingListPage": {
      "types": "./dist/cards/managed/PagingListPage.svelte.d.ts",
      "import": "./dist/cards/managed/PagingListPage.svelte"
    },
    "./CommonFormPage": {
      "types": "./dist/form-pages/CommonFormPage.svelte.d.ts",
      "import": "./dist/form-pages/CommonFormPage.svelte"
    },
    "./data-table": {
      "types": "./dist/data-table/index.d.ts",
      "import": "./dist/data-table/index.js"
    },
    "./data-table/DataTableBoard": {
      "types": "./dist/data-table/index.d.ts",
      "import": "./dist/data-table/index.js"
    },
    "./data-table/ListPage": {
      "types": "./dist/data-table/ListPage.svelte.d.ts",
      "import": "./dist/data-table/ListPage.svelte"
    },
    "./data-table/PagingListPage": {
      "types": "./dist/data-table/PagingListPage.svelte.d.ts",
      "import": "./dist/data-table/PagingListPage.svelte"
    },
    "./data-table/managed/ListPage": {
      "types": "./dist/data-table/managed/ListPage.svelte.d.ts",
      "import": "./dist/data-table/managed/ListPage.svelte"
    },
    "./data-table/managed/PagingListPage": {
      "types": "./dist/data-table/managed/PagingListPage.svelte.d.ts",
      "import": "./dist/data-table/managed/PagingListPage.svelte"
    },
    "./data-table/managed/DynamicPagingListPage": {
      "types": "./dist/data-table/managed/DynamicPagingListPage.svelte.d.ts",
      "import": "./dist/data-table/managed/DynamicPagingListPage.svelte"
    },
    "./NotInFramePage.svelte": {
      "types": "./dist/common/NotInFramePage.svelte.d.ts",
      "import": "./dist/common/NotInFramePage.svelte"
    },
    "./module": {
      "types": "./dist/module/index.d.ts",
      "import": "./dist/module/index.js"
    },
    "./module/ModuleHome": {
      "types": "./dist/module/ModuleHome.svelte.d.ts",
      "import": "./dist/module/ModuleHome.svelte"
    },
    "./multiple-modules": {
      "types": "./dist/multiple-modules/index.d.ts",
      "import": "./dist/multiple-modules/index.js"
    },
    "./multiple-modules/TabModules": {
      "types": "./dist/multiple-modules/TabModules.svelte.d.ts",
      "import": "./dist/multiple-modules/TabModules.svelte"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@ticatec/app-data-manager": "^1.0.7",
    "@ticatec/app-data-service": "^1.0.4",
    "@ticatec/axios-restful-service": "^0.1.7",
    "@ticatec/enhanced-utils": "^0.9.0",
    "@ticatec/i18n": "^0.2.0",
    "@ticatec/uniface-dev-shell": "^0.0.6",
    "@ticatec/uniface-element": "^0.2.0",
    "@ticatec/uniface-filter-panel": "^0.0.10",
    "@ticatec/uniface-google-material-icons": "^0.1.2",
    "@ticatec/uniface-icons": "^0.2.0",
    "axios": "^1.8.2",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.7",
    "publint": "^0.3.2",
    "sass": "^1.57.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-portal": "^2.2.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.3.1",
    "typescript": "^5.7.3",
    "vite": "^5.4.11"
  },
  "author": "Henry Feng",
  "license": "MIT",
  "private": false
}
